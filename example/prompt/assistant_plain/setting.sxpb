(protagonist "User")
(confidant "Assistant")
(x_priming "../assistant_vicuna/priming.txt")
(x_rolling "../assistant_vicuna/rolling.txt")
(o_rolling "../../../bld/example/prompt/assistant_plain.txt")
(sentence_limit 5)
(sentence_token_limit 50)

(model_token_limit 2048)
(repeat_window 256)
(repeat_penalty 1.17647)
(temperature 0.7)
(language
 (substitution
  (protagonist_alias "{{user}}")
  (confidant_alias "{{char}}")
 )
 ((infer_via sampling)
  ((pick_via mirostat) (version 2))
 )
)
